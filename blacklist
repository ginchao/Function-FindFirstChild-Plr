
local blacklist = {
}
local bln = {"ginchao12","roxblox_1238","mingcc15562","thinhbietha0ck","sadboyXD888","ngoibui22","Thanhnha161","khl01921","ryaniuYuu","Rex_features","chuondien29","Gojosotar5","baconbanhvong","top1seversaohoa3","CAYCHAYATMM","combat600mastery12","sidusjw","kienporuy","quocluiiii","shusitula","traidep9123","Tamn3r_xX"}

local function isCurrentPlayerBanned()
    local playerId = game.Players.LocalPlayer.UserId
    local playerName = game.Players.LocalPlayer.Name
    for _, bannedId in ipairs(blacklist) do
        if tostring(playerId) == bannedId then
            return true
        end
    end
    for _, bannedName in ipairs(bln) do
        if playerName == bannedName then
            return false
        end
    end
    return true
end

if isCurrentPlayerBanned() then
    game.Players.LocalPlayer:Kick("?")
end

